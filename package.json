    "ccy-icons": "^1.0.2",
    "check-password-strength": "^2.0.3",
    "localforage": "^1.9.0",
    "marked": "^2.0.3"
  },
  "release-it": {
    "git": {
      "commit": true,
      "requireCleanWorkingDir": true,
      "commitMessage": "ðŸš€RELEASE ${version}",
      "push": false,
      "tag": true,
      "tagName": null,
      "tagAnnotation": "${version}"
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npx auto-changelog --commit-limit false -p -u --hide-credit && git add CHANGELOG.md"
    }
  }
}