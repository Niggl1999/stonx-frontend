{
  "name": "stonkx-frontend",
  "version": "0.2.5",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "rimraf ./build ./svelte-kit ./svelte && svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "release": "release-it"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.10",
    "@sveltejs/kit": "1.0.0-next.108",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "autoprefixer": "10.2.5",
    "cssnano": "5.0.4",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.0",
    "postcss": "8.3.0",
    "postcss-load-config": "3.0.1",
    "prettier": "~2.3.0",
    "prettier-plugin-svelte": "2.3.0",
    "release-it": "14.6.2",
    "rimraf": "3.0.2",
    "svelte": "3.38.2",
    "svelte-preprocess": "4.7.3",
    "svelte-select": "3.17.0",
    "svelte-use-click-outside": "1.0.0",
    "tailwindcss": "2.1.2",
    "tslib": "2.2.0",
    "typescript": "4.2.4",
    "vite": "2.3.3"
  },
  "type": "module",
  "dependencies": {
    "@sotatek-anhdao/coin-address-validator": "1.0.1",
    "axios": "0.21.1",
    "base64-cryptocurrency-icons": "1.1.17",
    "chart.js": "3.3.0",
    "check-password-strength": "2.0.3",
    "localforage": "1.9.0",
    "marked": "2.0.5"
  },
  "release-it": {
    "git": {
      "commit": true,
      "requireCleanWorkingDir": true,
      "commitMessage": "ðŸš€RELEASE ${version}",
      "push": false,
      "tag": true,
      "tagName": null,
      "tagAnnotation": "${version}"
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npx auto-changelog --commit-limit false -p -u --hide-credit && git add CHANGELOG.md"
    }
  }
}
