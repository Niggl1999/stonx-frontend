    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "2.1.0"
  },
  "type": "module",
  "dependencies": {
    "@sotatek-anhdao/coin-address-validator": "^1.0.1",
    "axios": "^0.21.1",
